{
  "name": "pms-backend",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon --exec ts-node ./src/server.ts",
    "migrate": "node -e \"require('dotenv').config(); var env = process.env.NODE_ENV || 'development'; require('child_process').execSync('sequelize db:migrate --env ' + env, { stdio: [0, 1, 2] });\"",
    "seed": "node -e \"require('dotenv').config(); var env = process.env.NODE_ENV || 'development'; require('child_process').execSync('sequelize db:seed:all --env ' + env, { stdio: [0, 1, 2] });\"",
    "build": "rm -rf ./dist && tsc -p .",
    "start": "node ./dist/server.js",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:start": "docker-compose up",
    "docker:down": "docker-compose down"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.0",
    "@types/nodemailer": "^7.0.3",
    "nodemon": "^3.1.10",
    "rimraf": "^6.1.0",
    "sequelize-cli": "^6.6.3",
    "ts-node": "^10.9.2"
  },
  "dependencies": {
    "@types/socket.io": "^3.0.2",
    "bcryptjs": "^3.0.3",
    "cloudinary": "^2.8.0",

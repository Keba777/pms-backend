{
  "name": "pms-backend",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon --exec ts-node ./src/server.ts",
    "migrate": "node -e \"require('dotenv').config(); var env = process.env.NODE_ENV || 'development'; require('child_process').execSync('sequelize db:migrate --env ' + env, { stdio: [0, 1, 2] });\"",
    "seed": "node -e \"require('dotenv').config(); var env = process.env.NODE_ENV || 'development'; require('child_process').execSync('sequelize db:seed:all --env ' + env, { stdio: [0, 1, 2] });\"",
    "build": "rm -rf ./dist && tsc -p .",
    "start": "npm run migrate && node ./dist/server.js",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:start": "docker-compose up",
    "docker:down": "docker-compose down"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.13.10",
    "nodemon": "^3.1.9",
    "rimraf": "^6.0.1",
    "sequelize-cli": "^6.6.2",
    "ts-node": "^10.9.2"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cloudinary": "^2.6.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.14.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.6",
    "sequelize-typescript": "^2.1.6",
    "typescript": "^5.8.2"
  }
}
